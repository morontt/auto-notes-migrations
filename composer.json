{
    "name": "xelbot/migrations-auto-notes",
    "license": "Zlib",
    "type": "project",
    "autoload": {
        "psr-4": {
            "AutoNotes\\": ""
        }
    },
    "require": {
        "php": ">=8.1",
        "ext-pdo_mysql": "*",
        "doctrine/inflector": "^2.0",
        "doctrine/migrations": "^3.7",
        "doctrine/orm": "^3.1",
        "symfony/cache": "^6.4",
