    },
    "conflict": {
        "symfony/event-dispatcher": ">=7.0",
        "symfony/filesystem": ">=7.0",
        "symfony/finder": ">=7.0",
        "symfony/options-resolver": ">=7.0",
        "symfony/process": ">=7.0",
        "symfony/stopwatch": ">=7.0",
        "symfony/string": ">=7.0",
        "symfony/var-exporter": ">=7.0"
